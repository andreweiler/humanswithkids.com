<div class="row top">
  <div class="post-content">
    <!-- <h1><%= current_user.name %></h1> -->
    
    <%= link_to 'New Post', new_user_post_path(current_user.id), class: "btn btn-primary" %>
  
    
    <table id="all-posts" class="table">
     <% @posts.each do |post| %>
       <tr class="<%= post.published? ? "published" : "private" %>">
         <td>
           <%= link_to post.title, blogpost_path(post) %></br>
           <%= post.published_on %>
         </td>
         <td>
           <%= link_to "Edit", edit_user_post_path(current_user, post) %></br>
           <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %>
         </td>
       </tr>
     <% end %>
    </table>
    
  </div>
</div><!--/row-->
